  "description": "Global Site Tag (gtag.js) plugin for Docusaurus.",
  "main": "lib/index.js",
  "types": "src/plugin-google-gtag.d.ts",
  "scripts": {
    "build": "yarn build:server && yarn build:browser && yarn build:copy && yarn build:format",
