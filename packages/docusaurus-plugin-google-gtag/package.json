    "clsx": "^1.1.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@docusaurus/types": "2.0.0-beta.14"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0"
